DATA ATB_2;
	SET dw.vw_ATB_Data(
		KEEP = BRACCTNO Age2 YEARMONTH);
	AGE2 = Age2;
	YEARMONTH = YEARMONTH;
	BRACCTNO = BRACCTNO;
	POACCTNO = BRACCTNO * 1;
	
	ATBDT = INPUT(SUBSTR(YEARMONTH, 6, 2) || '/' ||
				  SUBSTR(YEARMONTH, 9, 2) || '/' ||
				  SUBSTR(YEARMONTH, 1, 4), mmddyy10.);
	format ATBDT mmddyy10.;
	IF AGE2 =: '1' THEN AGE2 = '1.Current';
	KEEP ATBDT AGE2 BRACCTNO;
