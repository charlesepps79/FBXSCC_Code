	IF AGE_OPEN < 7 OR AGE_CLOSED < 7 THEN DO;
		IF CD > 2 THEN RECENT6 = 1; *NOTE 30-59S IN LAST Six MONTHS;
		IF CD > 3 THEN RECENT6_60 = 1;
