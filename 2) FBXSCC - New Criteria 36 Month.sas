	ELSE IF AGE_OPEN =13 OR AGE_CLOSED = 13 THEN DELQ13= CD;
	ELSE IF AGE_OPEN =14 OR AGE_CLOSED = 14 THEN DELQ14= CD;
	ELSE IF AGE_OPEN =15 OR AGE_CLOSED = 15 THEN DELQ15= CD;
	*** IF CD IS GREATER THAN 30-59 DAYS LATE, SET CD60 TO 1 ----- ***;
	IF CD > 3 THEN CD60 = 1; 
	*** IF CD IS GREATER THAN 1-29 DAYS LATE, SET CD30 TO 1 ------ ***;
