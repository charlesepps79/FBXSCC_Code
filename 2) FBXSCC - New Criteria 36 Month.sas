	IF OWNBR = "0668" THEN OWNBR = "0680";

	IF "&_18MONTH" <= POFFDATE < "&_15MONTH" THEN _18MTH_SWAPIN = "X";
	IF "&_24MONTH" <= POFFDATE < "&_18MONTH" THEN _24MTH_SWAPIN = "X";
