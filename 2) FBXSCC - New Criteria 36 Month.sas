DATA ATB_1;
	SET dw.vw_AgedTrialBalance(
		KEEP = LoanNumber Age2 BOM RUNDATE);
	AGE2 = Age2;
	YEARMONTH = BOM;
	BRACCTNO = LoanNumber;
	POACCTNO = BRACCTNO * 1;
	
	ATBDT = INPUT(SUBSTR(YEARMONTH, 6, 2) || '/' ||
				  SUBSTR(YEARMONTH, 9, 2) || '/' ||
				  SUBSTR(YEARMONTH, 1, 4), mmddyy10.);
	format ATBDT mmddyy10.;
	IF AGE2 =: '1' THEN AGE2 = '1.Current';
	KEEP ATBDT AGE2 BRACCTNO BOM RunDate;
