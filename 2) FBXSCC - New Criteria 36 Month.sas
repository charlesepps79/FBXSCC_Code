		*** NOTE 30-59S FROM 7 TO 15 MONTHS AGO ------------------ ***;
		IF CD > 2 THEN FIRST6 = 1;
		IF CD > 3 THEN FIRST6_60 = 1;
	END;
	KEEP BRACCTNO DELQ1-DELQ15 CD CD30 CD60 AGE2 ATBDT AGE_OPEN
